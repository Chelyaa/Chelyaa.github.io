{
  "name": "BOT for pernatsk",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "content.js" ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "injected.js"
  ]
}