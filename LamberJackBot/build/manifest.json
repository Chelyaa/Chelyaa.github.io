 {
  "manifest_version": 2,
  "name": "LamberJackBot",
  "version": "1.0",
  
  "web_accessible_resources": [
    
  ],
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "content.js" ],
      "run_at": "document_end"
    }
  ],
  "background": {
    // "scripts": ["background.js"]
  },
  "permissions": [
    "notifications"
  ],
  "browser_action": {
    "default_title": "LamberJackBot",
    "default_icon": "128.png",
    "default_popup": "popup.html"
  }
}